{{#each card_list}}
<div class="panel panel-primary">
 <div class="panel-heading">
  <h3 class="panel-title">{{card_detail.cash.base_info.brand_name}}</h3>
 </div>
 <div class="panel-body">
  <h3 class="text-center text-primary"><strong>{{card_detail.cash.base_info.title}}</strong><h3>
  <a href="/wxcard/consume?openid={{openid}}&code={{code}}&card_id={{card_id}}" class="btn btn-primary btn-lg btn-block active" role="button">立即使用</button></a>
  <h5 class="text-center text-muted">{{card_detail.cash.base_info.sub_title}}<h5>
 </div>
</div>
{{else}} <h3 class="text-center text-primary"><strong>没有相关数据</strong><h3>
{{/each}}
